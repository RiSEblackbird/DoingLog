<div class='user_card'>
  <ul>ID: <%= user.id %></ul>
  <ul>Name: <%= user.username %></ul>
  <ul>Profile: </ul>
  <ul> <%= user.profile %></ul>
  <div id="user_btn">
    <% if user_signed_in? %>
      <% if current_user %>
        <%= link_to "ユーザー情報の編集", edit_user_registration_path, class: 'btn btn-sm btn-secondary btn-block' ,id: 'edit_btn' %>
      <% end %>
    <% end %>
  </div>
</div>